{
  "name": "feedbackhub-on-eks",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:build": "docker build -f docker/Dockerfile.dev -t feedbackhub:latest .",
    "docker:build:prod": "docker build -f docker/Dockerfile.prod -t feedbackhub:prod .",
    "docker:build:ecr": "npm run build && docker build -f docker/Dockerfile.prod -t feedbackhub:$(git rev-parse --short HEAD) .",
    "docker:dev": "docker-compose -f docker/docker-compose.dev.yml up --build",
    "docker:dev:down": "docker-compose -f docker/docker-compose.dev.yml down"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.864.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-toast": "^1.1.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.263.1",
    "mongodb": "^6.0.0",
    "next": "14.2.31",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.31",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
  }
